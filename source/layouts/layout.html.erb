<!doctype html>
<html class="no-js">
   <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "This Developing Journey" %></title>
    
    <meta name="description" content="Discoveries into Go, JavaScript, and Ruby">
    <meta name="viewport" content="width=device-width">

    <!-- Extra Meta-->
    <!-- Twitter Card data -->
    <meta name="twitter:site" content="@bdougYO">
    <meta name="twitter:title" content="This Developing Journey">
    <meta name="twitter:creator" content="@bdougieYO">
    <meta name="twitter:image" content="https://pbs.twimg.com/profile_images/378800000866149076/IQhqAfva.jpeg">

    <!-- Open Graph data -->
    <meta property="og:title" content="This Developing Journey" />
    <meta property="og:type" content="article" />
    <meta property="og:url" content="http://blog.briandouglas.me" />
    <meta property="og:image" content="http://netlify.scdn4.secure.raxcdn.com/images/ced071bb56f55694eeb9d01d3c0a35d3b561d38d/brian.jpg" />
    <meta property="og:description" content="Discoveries into Go, JavaScript, and Ruby" /> 
    <meta property="og:site_name" content="This Developing Journey" />

    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="stylesheet" href="css/font-awesome.min.css">
    <%= stylesheet_link_tag "css/main" %>

    <!--[if lt IE 9]>
            <script src="/js/vendor/html5shiv.min.js"></script>
        <![endif]-->

    <script src="js/vendor/modernizr.custom.32229-2.8-respondjs-1-4-2.js"></script>
  </head>
  
  <body class="<%= page_classes %>">
    <%= yield %>
    <%= partial "layouts/footer" %>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="js/vendor/jquery-1.11.2.min.js"><\/script>')</script>

    <script src="js/vendor/jquery.jpanelmenu.min.js"></script>
    <script src="js/vendor/bootstrap.min.js"></script>
    <script src="js/vendor/fastclick.min.js"></script>

    <script src="js/main.js"></script>

  </body>
</html>
